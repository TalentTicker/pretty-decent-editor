{
   "extends": "./tsconfig.base.json",
   "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "esModuleInterop": true,
    "module": "ESNext",
    "downlevelIteration": true,
    "emitDeclarationOnly": true
   },
    "include": [
      "./src"
    ],
    "exclude": [
      "node_modules",
      "*.test.*",
      "dist"
    ]
  }